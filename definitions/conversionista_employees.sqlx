config {
    type: "table", 
    schema: "c_forecast_dev",
    name: "c-employees"
}

SELECT
    username, 
    name, 
    employment_id,
    country, 
    location, 
    seniority
FROM c-forecast-generation.happy_people 
WHERE company LIKE '%Conversionista%' 
      AND employment_status = 'Employed'